<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >


<web-app>

  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>NovaEmpresaServlet</servlet-name>
  	<display-name>NovaEmpresaServlet</display-name>
  	<description></description>
  	<servlet-class>apploja.NovaEmpresaServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ListaEmpresasServlet</servlet-name>
  	<display-name>ListaEmpresasServlet</display-name>
  	<description></description>
  	<servlet-class>apploja.ListaEmpresasServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>NovaEmpresaServlet</servlet-name>
  	<url-pattern>/novaempresa</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ListaEmpresasServlet</servlet-name>
  	<url-pattern>/listaempresas</url-pattern>
  </servlet-mapping>
</web-app>
